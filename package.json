    "mocha": "^9.1.1",
    "pg": "8.6.0",
    "pg-hstore": "2.3.4",
    "sequelize": "6.6.5",
    "sequelize-cli": "6.2.0",
    "sequelize-noupdate-attributes": "1.0.0"
  }
}
